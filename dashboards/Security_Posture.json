          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \r\n    severity_levels.severity,\r\n    COALESCE(severity_data.severity_count, 0) AS severity_count\r\nFROM (\r\n    SELECT 'High' AS severity\r\n    UNION ALL SELECT 'Medium'\r\n    UNION ALL SELECT 'Low'\r\n    UNION ALL SELECT 'Info'\r\n    UNION ALL SELECT 'Total'\r\n) AS severity_levels\r\nLEFT JOIN (\r\n    SELECT \r\n        f.severity,\r\n        COUNT(*) AS severity_count\r\n    FROM \r\n        findings f\r\n    JOIN \r\n        builds b ON f.build_id = b.id\r\n    WHERE \r\n        b.app = '${AppName}'\r\n        AND b.build_number IN (\r\n            SELECT build_number FROM (\r\n                (SELECT build_number \r\n                 FROM builds \r\n                 WHERE app = '${AppName}' AND build_number LIKE 'CI_%'\r\n                 ORDER BY build_number DESC LIMIT 1)\r\n                UNION\r\n                (SELECT build_number \r\n                 FROM builds \r\n                 WHERE app = '${AppName}' AND build_number LIKE 'CD_%'\r\n                 ORDER BY build_number DESC LIMIT 1)\r\n            ) AS latest_builds\r\n        )\r\n        AND f.severity IN (${severity})\r\n        AND f.toolname IN (\r\n            'Dependency Check Scan',\r\n            'SonarQube',\r\n            'Android Security Assessment',\r\n            'iOS Security Assessment',\r\n            'Trufflehog3 Scan',\r\n            'ZAP Scan'\r\n        )\r\n        AND b.module = '${repo_name}'\r\n        AND f.is_new IN (${New})\r\n        AND f.suppressed IN (${Suppressed})\r\n    GROUP BY f.severity\r\n\r\n    UNION ALL\r\n\r\n    SELECT \r\n        'Total' AS severity,\r\n        COUNT(*) AS severity_count\r\n    FROM \r\n        findings f\r\n    JOIN \r\n        builds b ON f.build_id = b.id\r\n    WHERE \r\n        b.app = '${AppName}'\r\n        AND b.build_number IN (\r\n            SELECT build_number FROM (\r\n                (SELECT build_number \r\n                 FROM builds \r\n                 WHERE app = '${AppName}' AND build_number LIKE 'CI_%'\r\n                 ORDER BY build_number DESC LIMIT 1)\r\n                UNION\r\n                (SELECT build_number \r\n                 FROM builds \r\n                 WHERE app = '${AppName}' AND build_number LIKE 'CD_%'\r\n                 ORDER BY build_number DESC LIMIT 1)\r\n            ) AS latest_builds\r\n        )\r\n        AND f.severity IN (${severity})\r\n        AND f.toolname IN (\r\n            'Dependency Check Scan',\r\n            'SonarQube',\r\n            'Android Security Assessment',\r\n            'iOS Security Assessment',\r\n            'Trufflehog3 Scan',\r\n            'ZAP Scan'\r\n        )\r\n        AND b.module = '${repo_name}'\r\n        AND f.is_new IN (${New})\r\n        AND f.suppressed IN (${Suppressed})\r\n) AS severity_data ON severity_levels.severity = severity_data.severity\r\nORDER BY \r\n    CASE \r\n        WHEN severity_levels.severity = 'High' THEN 1\r\n        WHEN severity_levels.severity = 'Medium' THEN 2\r\n        WHEN severity_levels.severity = 'Low' THEN 3\r\n        WHEN severity_levels.severity = 'Info' THEN 4\r\n        WHEN severity_levels.severity = 'Total' THEN 5\r\n        ELSE 6\r\n    END;\r\n",
