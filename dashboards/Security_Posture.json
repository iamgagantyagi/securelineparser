          "rawQuery": true,
          "rawSql": "SELECT\r\n    severity_levels.severity AS severity,\r\n    COALESCE(findings_data.severity_count, 0) AS severity_count\r\nFROM\r\n    (\r\n        SELECT 'High' AS severity UNION ALL\r\n        SELECT 'Medium' UNION ALL\r\n        SELECT 'Low' UNION ALL\r\n        SELECT 'Info' UNION ALL\r\n        SELECT 'Total'\r\n    ) AS severity_levels\r\nLEFT JOIN\r\n    (\r\n        SELECT\r\n            f.severity AS severity,\r\n            COUNT(*) AS severity_count\r\n        FROM\r\n            findings f\r\n        JOIN\r\n            builds b ON f.build_id = b.id\r\n        WHERE\r\n            b.app = '${AppName}'\r\n            AND b.build_number IN (\r\n                SELECT build_number FROM (\r\n                    (SELECT build_number FROM builds \r\n                     WHERE app = '${AppName}' AND build_number LIKE 'CI_%' \r\n                     ORDER BY build_number DESC LIMIT 1)\r\n                    UNION\r\n                    (SELECT build_number FROM builds \r\n                     WHERE app = '${AppName}' AND build_number LIKE 'CD_%' \r\n                     ORDER BY build_number DESC LIMIT 1)\r\n                ) AS latest_builds\r\n            )\r\n            AND f.severity IN (${severity})\r\n            AND b.module = '${repo_name}'\r\n            AND f.is_new IN (${New})\r\n            AND f.suppressed IN (${Suppressed})\r\n            AND f.toolname IN (\r\n                'Trivy_MisConfig',\r\n                'Trivy Image Scan',\r\n                'Trivy_CIS',\r\n                'Kubescape Scanning_CI',\r\n                'Kubescape Scanning_CD',\r\n                'AWS Cloud Security Audit',\r\n                'UBUNTU20-CIS-Audit'\r\n            )\r\n        GROUP BY\r\n            f.severity\r\n       \r\n        UNION ALL\r\n       \r\n        SELECT\r\n            'Total' AS severity,\r\n            COUNT(*) AS severity_count\r\n        FROM\r\n            findings f\r\n        JOIN\r\n            builds b ON f.build_id = b.id\r\n        WHERE\r\n            b.app = '${AppName}'\r\n            AND b.build_number IN (\r\n                SELECT build_number FROM (\r\n                    (SELECT build_number FROM builds \r\n                     WHERE app = '${AppName}' AND build_number LIKE 'CI_%' \r\n                     ORDER BY build_number DESC LIMIT 1)\r\n                    UNION\r\n                    (SELECT build_number FROM builds \r\n                     WHERE app = '${AppName}' AND build_number LIKE 'CD_%' \r\n                     ORDER BY build_number DESC LIMIT 1)\r\n                ) AS latest_builds\r\n            )\r\n            AND f.severity IN (${severity})\r\n            AND b.module = '${repo_name}'\r\n            AND f.is_new IN (${New})\r\n            AND f.suppressed IN (${Suppressed})\r\n            AND f.toolname IN (\r\n                'Trivy_MisConfig',\r\n                'Trivy Image Scan',\r\n                'Trivy_CIS',\r\n                'Kubescape Scanning_CI',\r\n                'Kubescape Scanning_CD',\r\n                'AWS Cloud Security Audit',\r\n                'UBUNTU20-CIS-Audit'\r\n            )\r\n    ) AS findings_data ON severity_levels.severity = findings_data.severity\r\nORDER BY\r\n    CASE\r\n        WHEN severity_levels.severity = 'High' THEN 1\r\n        WHEN severity_levels.severity = 'Medium' THEN 2\r\n        WHEN severity_levels.severity = 'Low' THEN 3\r\n        WHEN severity_levels.severity = 'Info' THEN 4\r\n        WHEN severity_levels.severity = 'Total' THEN 5\r\n        ELSE 6\r\n    END;\r\n",
