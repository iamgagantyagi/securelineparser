        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \r\n    f.toolname AS \"Tool\", \r\n    f.title AS \"Tittle\", \r\n    f.description AS \"Description\", \r\n    f.filepath AS \"Filepath\", \r\n    f.line AS \"Line#\", \r\n    f.remediation AS \"Remediation\", \r\n    f.severity AS \"Severity\" \r\nFROM findings f \r\nJOIN builds b ON f.build_id = b.id \r\nWHERE b.app = '${AppName}' \r\nAND b.build_number = '${ScanName}' \r\nAND f.severity IN (${severity}) \r\nAND (\r\n    '${New:raw}' = '{true,false}' \r\n    OR f.is_new::text = ANY(string_to_array('${New:raw}', ','))\r\n)\r\nAND (\r\n    '${Suppressed:raw}' = '{true,false}' \r\n    OR f.suppressed::text = ANY(string_to_array('${Suppressed:raw}', ','))\r\n)\r\n",
